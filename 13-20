//17

import math

A=5.0
H=2.0
R=3.0
M=10.0
CV=A**3
CYV=math.pi * R**2 * H
if CV>M:
 print ("possibly")

else:

 print ("unpossibly")

if CYV>M:

 print ("possibly")

else:

 print ("unpossibly")

if M<CYV<CV:

 print ("possibly")

else:

 print ("unpossibly")

//18

import random
import math 
A = random.randint (1,5)

V = A**3

print ("Объем куба",V)Объем куба"Объем куба",V),V)

P = math.pi
H = random.randint ( 1,5)
R = random.randint ( 1,3)

W = (P*R**2)*H
print ("Объем куба",V)Объем цилиндра"Объем куба",V),W)
Vg = random.randint ( 1,50)
print ("Объем куба",V)Объем жидкости"Объем куба",V),Vg)

if Vg > V :

 print ("Объем куба",V)Можно заполнить куб"Объем куба",V))
else :
 print ("Объем куба",V)Нельзя заполнить куб"Объем куба",V))

if Vg > W :

 print ("Объем куба",V)Можно заполнить цилиндр"Объем куба",V))
else :
 print ("Объем куба",V)Нельзя заполнить цилиндр"Объем куба",V))

if Vg > V+W :

 print ("Объем куба",V)можно заполнить обе "Объем куба",V))
else :
 print ("Объем куба",V)нельзя заполнить обе"Объем куба",V))

//19

X=6.0
Y=5.0
Z=7.0
if X+Y>Z and X-Y<Z:

 print ("exists")
else:
 print("does not exist")

if Z**2 == X**2+Y**2:

 print ("rectangular")
else:
 print ("not rectangular")

