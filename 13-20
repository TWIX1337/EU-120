p=0

ost=0

print(a)

if a%2 ==0:

 if a%500==0:

 print(str(a/500)- " - po 500")

 else:

 P=str (a//500)

 ost=a%500

 P=(P+"-po 500")

 print (P)

 if ost%100==0:

 S=ost/100

 print(S)

 else:

 S=str(ost//100)
 ost=ost%100

 S=(S +"-po 100")

 print(S)

 if ost%10==0:
 print(str(ost/10)+"-po 10")
 else:
 D=str(ost//10)
 ost=ost%10
 D=(D+"- po 10")
 print(D)
